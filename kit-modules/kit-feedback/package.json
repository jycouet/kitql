{
	"name": "@kitql/kit-feedback",
	"version": "0.0.2",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jycouet/kitql",
		"directory": "packages/modules/kit-feedback"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.js"
		},
		"./*": {
			"require": "./dist/*.js",
			"import": "./dist/*.js"
		}
	},
	"typings": "dist/index.d.ts",
	"typescript": {
		"definition": "dist/index.d.ts"
	},
	"buildOptions": {
		"input": "./src/lib/index.ts",
		"output": "./dist/index.js"
	},
	"scripts": {
		"dev": "env-cmd svelte-kit dev --port 3001",
		"build-kit": "svelte-kit build",
		"build": "tsc --project tsconfig.json",
		"gen": "graphql-codegen --config ./.graphqlrc.yaml",
		"gen:style": "npx tailwindcss -c ./tailwind.config.cjs -o ./dist/app.postcss --minify",
		"package": "svelte-kit package && yarn gen:style",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@felte/validator-vest": "^1.0.6",
		"@iconify/svelte": "^2.2.1",
		"@kitql/graphql-codegen": "^0.5.1",
		"@kitql/vite-plugin-watch-and-run": "^0.3.0",
		"@playwright/test": "^1.20.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/lodash": "^4.14.182",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.4",
		"cssnano": "^5.1.7",
		"daisyui": "^2.14.1",
		"env-cmd": "^10.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"felte": "^1.1.7",
		"lodash": "^4.17.21",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.5",
		"svelte2tsx": "^0.5.8",
		"tailwind-scrollbar": "^1.3.1",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2",
		"vest": "^4.3.3"
	},
	"dependencies": {
		"@kitql/client": "^0.5.3",
		"graphql": "^16.3.0"
	},
	"peerDependencies": {},
	"type": "module",
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	}
}
