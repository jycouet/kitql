{
  "name": "@kitql/helpers",
  "description": "Some useful helpers (0 deps)",
  "keywords": [
    "svelte",
    "terminal",
    "styles",
    "color",
    "ansi",
    "log"
  ],
  "version": "0.8.10",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/helper",
    "homepage": "https://github.com/jycouet/kitql/tree/main/packages/helpers"
  },
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "vite build && svelte-package && node ../../scripts/package.js",
    "preview": "vite preview",
    "package": "svelte-package && publint",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "kitql-lint",
    "format": "kitql-lint --format"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "3.3.1",
    "@sveltejs/kit": "2.15.1",
    "@sveltejs/package": "2.3.7",
    "@kitql/eslint-config": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "publint": "0.2.4",
    "rollup-plugin-visualizer": "5.12.0",
    "svelte": "5.16.0",
    "svelte-check": "4.1.1",
    "tslib": "2.8.0",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "esm-env": "^1.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "require": "./cjs/index.js",
      "svelte": "./esm/index.js",
      "default": "./esm/index.js"
    }
  },
  "engines": {
    "node": "^16.14 || >=18"
  }
}