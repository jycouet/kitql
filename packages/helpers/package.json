{
  "name": "@kitql/helpers",
  "description": "Some useful helpers (0 deps)",
  "keywords": [
    "svelte",
    "terminal",
    "styles",
    "color",
    "ansi",
    "log"
  ],
  "version": "0.8.11",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/helper",
    "homepage": "https://github.com/jycouet/kitql/tree/main/packages/helpers"
  },
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "vite build && svelte-package && node ../../scripts/package.js",
    "preview": "vite preview",
    "package": "svelte-package && publint",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "TZ=UTC vitest",
    "test:ci": "TZ=UTC vitest run --coverage",
    "lint": "kitql-lint",
    "format": "kitql-lint --format"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "4.0.0",
    "@sveltejs/kit": "2.15.1",
    "@sveltejs/package": "2.3.7",
    "@kitql/eslint-config": "workspace:*",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "publint": "0.3.1",
    "rollup-plugin-visualizer": "5.14.0",
    "svelte": "5.18.0",
    "svelte-check": "4.1.1",
    "tslib": "2.8.0",
    "typescript": "5.7.2",
    "vite": "6.1.1",
    "vitest": "3.0.4"
  },
  "dependencies": {
    "esm-env": "^1.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "require": "./cjs/index.js",
      "svelte": "./esm/index.js",
      "default": "./esm/index.js"
    }
  },
  "engines": {
    "node": "^16.14 || >=18"
  }
}