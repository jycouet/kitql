{
  "name": "@kitql/eslint-config",
  "version": "0.3.7",
  "type": "module",
  "description": "opinionated linting and formatting for projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/eslint-config",
    "homepage": "https://github.com/jycouet/kitql/tree/main/packages/eslint-config"
  },
  "license": "MIT",
  "bin": {
    "kitql-lint": "./cmd.js"
  },
  "main": "index.cjs",
  "files": [
    ".eslintrc.cjs",
    ".prettierrc.cjs",
    "cmd.js",
    "cmd.sh",
    "index.cjs"
  ],
  "keywords": [
    "cli",
    "eslint",
    "eslint-config"
  ],
  "scripts": {
    "format": "node ./cmd.js -f",
    "format:example": "kitql-lint --format",
    "lint": "node ./cmd.js --verbose -p none",
    "lint:example": "kitql-lint"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@kitql/helpers": "workspace:*",
    "@theguild/eslint-config": "^0.12.1",
    "@theguild/prettier-config": "2.0.7",
    "commander": "^12.1.0",
    "eslint": "9.10.0",
    "globals": "^15.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
