{
  "name": "@kitql/template",
  "version": "0.3.1",
  "description": "KitQL template",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/template"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "node ./dist/index",
    "prepack": "bob prepack"
  },
  "devDependencies": {
    "tslib": "2.4.0"
  },
  "dependencies": {
    "yaml": "2.1.0"
  },
  "peerDependencies": {},
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "volta": {
    "node": "17.4.0",
    "yarn": "1.22.17"
  }
}
