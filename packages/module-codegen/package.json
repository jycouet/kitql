{
  "name": "@kitql/module-codegen",
  "version": "0.4.0-next.1",
  "description": "KitQL module-codegen",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/module-codegen"
  },
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cd ../.. && npm run build && cd packages/module-codegen && node ./dist/index.mjs",
    "start": "node ./dist/index.mjs",
    "build": "bob runify --single"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "tslib": "2.4.0"
  },
  "dependencies": {
    "@kitql/helper": "0.6.0-next.1",
    "glob": "^8.0.3",
    "yaml": "2.1.1"
  },
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "volta": {
    "node": "17.4.0"
  }
}
