{
  "name": "@kitql/handles",
  "description": "Some useful handles for SvelteKit",
  "keywords": [
    "svelte",
    "sveltekit"
  ],
  "version": "0.2.2",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/handles",
    "homepage": "https://github.com/jycouet/kitql/tree/main/packages/handles"
  },
  "scripts": {
    "build": "vite build && svelte-package && node ../../scripts/package.js",
    "check": "svelte-check",
    "check:watch": "svelte-check",
    "dev": "vite dev",
    "format": "kitql-lint --format",
    "lint": "kitql-lint",
    "package": "npm run build && publint",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "npm run test:e2e && npm run test",
    "test:e2e": "playwright test"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.4.0",
    "svelte": "^3.54.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@kitql/eslint-config": "workspace:*",
    "@playwright/test": "1.49.0",
    "@sveltejs/adapter-node": "5.2.2",
    "@sveltejs/kit": "2.15.1",
    "@sveltejs/package": "2.3.7",
    "@sveltejs/vite-plugin-svelte": "5.0.1",
    "@types/vary": "1.1.3",
    "publint": "0.2.4",
    "svelte": "5.16.0",
    "svelte-check": "4.1.1",
    "tslib": "2.8.0",
    "typescript": "5.7.2",
    "vite": "6.0.3",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@kitql/helpers": "workspace:*",
    "esm-env": "^1.0.0",
    "vary": "^1.1.2"
  },
  "sideEffects": false,
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "require": "./cjs/index.js",
      "svelte": "./esm/index.js",
      "default": "./esm/index.js"
    }
  },
  "engines": {
    "node": "^16.14 || >=18"
  }
}