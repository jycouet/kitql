{
  "name": "@kitql/handles",
  "description": "Some useful handles for SvelteKit",
  "version": "0.1.0-exp.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/handles",
    "homepage": "https://github.com/jycouet/kitql/tree/main/packages/handles"
  },
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "vite build && svelte-package && node ../../scripts/package.js",
    "preview": "vite preview",
    "package": "npm run build && publint",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest",
    "test:ci": "vitest --coverage",
    "lint": "kitql-lint",
    "format": "kitql-lint --fix"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.25.2",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "eslint-config-kitql": "workspace:*",
    "@sveltejs/adapter-node": "1.3.1",
    "@sveltejs/kit": "1.25.2",
    "@sveltejs/package": "2.2.2",
    "publint": "0.2.4",
    "svelte": "4.2.1",
    "svelte-check": "3.5.2",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.3.5",
    "vitest": "0.31.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js",
      "svelte": "./dist/index.js"
    }
  }
}
