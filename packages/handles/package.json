{
  "name": "@kitql/handles",
  "description": "Some useful handles for SvelteKit",
  "keywords": [
    "svelte",
    "sveltekit"
  ],
  "version": "0.1.6",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/handles",
    "homepage": "https://github.com/jycouet/kitql/tree/main/packages/handles"
  },
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "vite build && svelte-package && node ../../scripts/package.js",
    "preview": "vite preview",
    "package": "npm run build && publint",
    "check": "svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "kitql-lint",
    "format": "kitql-lint --format"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.4.0",
    "svelte": "^3.54.0 || ^4.0.0"
  },
  "devDependencies": {
    "@kitql/eslint-config": "workspace:*",
    "@kitql/helpers": "workspace:*",
    "@sveltejs/adapter-node": "^5.0.0",
    "@sveltejs/kit": "^2.4.0",
    "@sveltejs/package": "2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "publint": "0.2.4",
    "svelte": "4.2.1",
    "svelte-check": "3.8.0",
    "tslib": "2.6.2",
    "typescript": "5.4.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "esm-env": "^1.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "types": "./esm/index.d.ts",
      "default": "./esm/index.js",
      "svelte": "./esm/index.js"
    }
  },
  "engines": {
    "node": "^16.14 || >=18"
  }
}