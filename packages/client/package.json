{
	"name": "@kitql/client",
	"version": "0.5.12",
	"repository": {
		"type": "git",
		"url": "https://github.com/jycouet/kitql",
		"directory": "packages/client"
	},
	"license": "MIT",
	"scripts": {
		"prepare": "svelte-kit sync",
		"linkGen": "cd ../graphql-codegen/dist && yarn link && cd ../../client && yarn link @kitql/graphql-codegen",
		"gen": "graphql-codegen --config ./.graphqlrc.yaml",
		"dev": "svelte-kit dev",
		"buildKit": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"ts": "svelte-check --tsconfig ./tsconfig.json",
		"ts:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prepack": "bob prepack"
	},
	"devDependencies": {
		"@kitql/graphql-codegen": "0.5.4",
		"@kitql/module-codegen": "0.3.10",
		"@kitql/vite-plugin-watch-and-run": "0.4.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "1.0.0-next.358",
		"@typescript-eslint/eslint-plugin": "5.30.0",
		"@typescript-eslint/parser": "5.30.0",
		"eslint": "8.18.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"graphql-web-lite": "16.1.1000",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"svelte": "3.48.0",
		"svelte-check": "2.8.0",
		"svelte-preprocess": "4.10.7",
		"svelte2tsx": "0.5.11",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@kitql/helper": "0.3.5"
	},
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.js",
			"import": "./dist/*.mjs"
		}
	},
	"typings": "dist/index.d.ts",
	"typescript": {
		"definition": "dist/index.d.ts"
	},
	"buildOptions": {
		"input": "./src/lib/toExport/index.ts"
	},
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	}
}
