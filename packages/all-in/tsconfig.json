{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "NodeNext",
    "importsNotUsedAsValues": "preserve",
    "paths": {
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$graphql": ["src/lib/graphql"],
      "$graphql/*": ["src/lib/graphql/*"]
    }
  },
  "exclude": ["$kitql"]
}
