{
  "name": "@kitql/all-in",
  "version": "0.8.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/all-in"
  },
  "type": "module",
  "bob": false,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build && svelte-package -o dist",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/kit": "1.12.0",
    "@sveltejs/package": "^2.0.2",
    "@types/glob": "8.1.0",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.8.4",
    "prettier-plugin-svelte": "2.9.0",
    "svelte-check": "3.1.4",
    "svelte-preprocess": "5.0.2",
    "tslib": "2.5.0",
    "typescript": "5.0.2",
    "vite": "4.2.0"
  },
  "dependencies": {
    "@envelop/core": "^3.0.6",
    "@envelop/graphql-modules": "^4.0.6",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/graphql-modules-preset": "^3.1.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-resolvers": "^3.1.1",
    "@graphql-eslint/eslint-plugin": "^3.16.1",
    "@graphql-tools/graphql-tag-pluck": "^7.5.0",
    "@kitql/helper": "0.6.1",
    "@types/node": "^18.15.3",
    "@vue/compiler-sfc": "^3.2.47",
    "cosmiconfig": "^8.1.0",
    "cosmiconfig-toml-loader": "^1.0.0",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "glob": "^9.3.0",
    "graphql": "^16.6.0",
    "graphql-config": "^4.5.0",
    "graphql-modules": "^2.1.1",
    "graphql-scalars": "^1.20.4",
    "graphql-yoga": "3.7.3",
    "safe-stable-stringify": "^2.4.2",
    "svelte": "3.57.0",
    "svelte2tsx": "^0.6.10",
    "ts-node": "^10.9.1",
    "vite-plugin-watch-and-run": "1.1.2"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    "./cjs.cjs": "./dist/cjs.cjs",
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./offline": {
      "types": "./dist/offline.d.ts",
      "default": "./dist/offline.js"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "typesVersions": {
    ">4.0": {
      "index": [
        "./dist/index.d.ts"
      ],
      "offline": [
        "./dist/offline.d.ts"
      ]
    }
  }
}