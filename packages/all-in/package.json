{
  "name": "@kitql/all-in",
  "version": "0.9.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jycouet/kitql",
    "directory": "packages/all-in"
  },
  "type": "module",
  "bob": false,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build && svelte-package && node ./scripts/package.js",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@sveltejs/adapter-auto": "2.0.0",
    "@sveltejs/kit": "1.15.4",
    "@sveltejs/package": "^2.0.2",
    "@types/glob": "8.1.0",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.8.7",
    "prettier-plugin-svelte": "2.10.0",
    "svelte-check": "3.2.0",
    "svelte-preprocess": "5.0.3",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "vite": "4.2.1"
  },
  "dependencies": {
    "@envelop/core": "^3.0.6",
    "@envelop/graphql-modules": "^4.0.6",
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/graphql-modules-preset": "^3.1.2",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-resolvers": "^3.2.0",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@graphql-tools/graphql-tag-pluck": "^7.5.0",
    "@kitql/helper": "workspace:*",
    "@types/node": "^18.15.11",
    "@vue/compiler-sfc": "^3.2.47",
    "cosmiconfig": "^8.1.3",
    "cosmiconfig-toml-loader": "^1.0.0",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "glob": "^10.0.0",
    "graphql": "^16.6.0",
    "graphql-config": "^4.5.0",
    "graphql-modules": "^2.1.2",
    "graphql-scalars": "^1.21.3",
    "graphql-yoga": "3.8.0",
    "safe-stable-stringify": "^2.4.3",
    "svelte": "3.58.0",
    "svelte2tsx": "^0.6.11",
    "ts-node": "^10.9.1",
    "vite-plugin-watch-and-run": "workspace:*"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./cjs.cjs": "./cjs.cjs",
    "./offline": {
      "types": "./offline.d.ts",
      "default": "./offline.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index": [
        "./index.d.ts"
      ],
      "offline": [
        "./offline.d.ts"
      ]
    }
  }
}