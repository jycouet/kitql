    "remult": "^0.27.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-kit-routes": "0.6.6",
    "vite-plugin-stripper": "^0.5.0",
