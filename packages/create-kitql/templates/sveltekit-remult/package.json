    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "pg": "^8.11.3",
    "remult": "^0.24.1",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-kit-routes": "^0.5.0",
    "vite-plugin-stripper": "^0.3.2",
    "vitest": "^1.0.0"
