{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$houdini": ["$houdini"],
			"$graphql": ["src/lib/graphql"],
			"$graphql/*": ["src/lib/graphql/*"],
			"$modules": ["src/lib/modules"],
			"$modules/*": ["src/lib/modules/*"]
		},
		"rootDirs": [".", "./.svelte-kit/types", "./.$houdini/types"]
	}
}
