{
	"name": "demo-init",
	"version": "0.0.1",
	"license": "MIT",
	"description": "kitql demo init",
	"private": true,
	"repository": {
		"url": "https://github.com/jycouet/kitql",
		"directory": "examples/demo-init"
	},
	"type": "module",
	"scripts": {
		"dev": "svelte-kit sync && vite dev --port 3777",
		"build": "npm run build:date && vite build",
		"build:date": "./tools/preStart.sh",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@kitql/all-in": "0.8.5",
		"@kitql/helper": "0.6.1",
		"@playwright/test": "1.29.2",
		"@sveltejs/adapter-auto": "1.0.1",
		"@sveltejs/kit": "1.1.1",
		"@typescript-eslint/eslint-plugin": "5.48.2",
		"@typescript-eslint/parser": "5.48.2",
		"eslint": "8.32.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"graphql": "15.8.0",
		"graphql-sse": "2.0.0",
		"houdini": "0.20.1",
		"houdini-svelte": "0.20.1",
		"prettier": "2.8.3",
		"prettier-plugin-svelte": "2.9.0",
		"svelte": "3.55.1",
		"svelte-check": "3.0.2",
		"svelte-preprocess": "5.0.0",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"vite": "4.0.4"
	},
	"resolutions": {
		"graphql": "15.8.0"
	}
}
