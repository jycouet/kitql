{
	"name": "demo-init",
	"version": "0.0.1",
	"license": "MIT",
	"description": "kitql demo init",
	"private": true,
	"repository": {
		"url": "https://github.com/jycouet/kitql",
		"directory": "examples/demo-init"
	},
	"scripts": {
		"dev": "vite dev --port 3777",
		"build": "npm run build:date && vite build",
		"build:date": "./tools/preStart.sh",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@sveltejs/adapter-auto": "1.0.0-next.80",
		"@sveltejs/kit": "1.0.0-next.511",
		"@typescript-eslint/eslint-plugin": "5.30.7",
		"@typescript-eslint/parser": "5.30.7",
		"eslint": "8.20.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"svelte": "3.49.0",
		"svelte-check": "2.8.0",
		"svelte-preprocess": "4.10.7",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "3.1.7"
	},
	"dependencies": {
		"@kitql/all-in": "0.8.0-next.5",
		"houdini": "0.16.8",
		"graphql": "15.5.3"
	},
	"resolutions": {
		"@kitql/module-codegen": "0.4.0-next.1",
		"graphql": "15.5.3"
	},
	"type": "module",
	"engines": {
		"node": ">=16.0.0"
	},
	"volta": {
		"node": "17.4.0",
		"pnpm": "7.13.5"
	}
}
