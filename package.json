{
  "name": "kitql",
  "version": "0.0.1",
  "author": "jycouet <jycouet@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev:w": "cd website && npm run dev",
    "test:ci": "pnpm exec nx run-many --target=test:ci",
    "build": "rimraf packages/*/dist && pnpm exec nx run-many --target=build",
    "lint": "pnpm exec nx run-many --target=lint --exclude create-kitql-*",
    "format": "pnpm exec nx run-many --target=format",
    "release:version": "npm run changeset version",
    "release": "npm run build && changeset publish",
    "nx:graph": "pnpm exec nx graph",
    "nx:reset": "pnpm exec nx reset",
    "reset": "git clean -xdf",
    "up-dep": "pnpm -r up --latest"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.1",
    "@vitest/coverage-v8": "^0.34.6",
    "esbuild": "^0.19.5",
    "rimraf": "5.0.0",
    "nx": "17.2.0"
  }
}
