{
  "name": "kitql",
  "version": "0.0.1",
  "author": "jycouet <jycouet@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prepare": "husky install && cd packages/all-in && npm run prepare",
    "dev:w": "cd website && npm run dev",
    "test": "vitest --coverage",
    "lint": "eslint --ignore-path .prettierignore .",
    "prettier": "prettier --write --list-different .",
    "prettier:check": "prettier --check .",
    "test:ci": "vitest run --coverage",
    "test:update-badge": "npm run update-badge",
    "prebuild": "rimraf packages/*/dist",
    "build": "turbo run build",
    "release:version": "npm run changeset version",
    "release": "npm run build && changeset publish",
    "reset": "git clean -xdf",
    "turbo": "turbo run --color"
  },
  "workspaces": [
    "packages/*",
    "kit-modules/*",
    "website"
  ],
  "devDependencies": {
    "@changesets/changelog-github": "0.4.5",
    "@changesets/cli": "2.23.0",
    "@theguild/eslint-config": "0.0.2",
    "@theguild/prettier-config": "0.0.3",
    "@vitest/ui": "0.23.1",
    "bob-the-bundler": "4.0.0",
    "c8": "7.11.0",
    "check-code-coverage": "1.10.0",
    "eslint": "8.20.0",
    "husky": "8.0.1",
    "jsdom": "19.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "rimraf": "3.0.2",
    "svelte": "3.49.0",
    "svelte2tsx": "^0.5.16",
    "turbo": "^1.4.5",
    "typescript": "4.8.3",
    "vitest": "0.4.1"
  },
  "volta": {
    "node": "17.4.0"
  }
}
